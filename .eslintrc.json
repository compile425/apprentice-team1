{
    "env": {
      "browser": true,
      "es2021": true},
    "extends": ["eslint:recommended"],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "rules": {
      "no-console": "off",
      "indent": ["error", 2],
      "semi": ["error", "always"]
    }
  }